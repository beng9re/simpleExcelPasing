-- 테이블 순서는 관계를 고려하여 한 번에 실행해도 에러가 발생하지 않게 정렬되었습니다.

-- asd Table Create SQL
CREATE TABLE asd
(
    "a"      int            NOT NULL    IDENTITY, 
    "as"     varchar(50)    NULL, 
    "ass"    varchar(50)    NULL, 
    "asssa"  varchar(50)    NULL, 
    CONSTRAINT PK_ASD PRIMARY KEY (a)
)
GO

EXECUTE sp_addextendedproperty 'MS_Description',
    'q', 
   'user', dbo, 'table', 'asd', 'column', 'a'
GO

EXECUTE sp_addextendedproperty 'MS_Description',
    'qa', 
   'user', dbo, 'table', 'asd', 'column', 'as'
GO

EXECUTE sp_addextendedproperty 'MS_Description',
    'qa', 
   'user', dbo, 'table', 'asd', 'column', 'ass'
GO

EXECUTE sp_addextendedproperty 'MS_Description',
    'aa', 
   'user', dbo, 'table', 'asd', 'column', 'asssa'
GO


